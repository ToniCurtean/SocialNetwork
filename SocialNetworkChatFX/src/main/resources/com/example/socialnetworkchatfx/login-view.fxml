<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.socialnetworkchatfx.controller.LoginController">
   <children>
      <TextField fx:id="emailText" layoutX="404.0" layoutY="119.0" />
      <PasswordField fx:id="passwordText" layoutX="401.0" layoutY="187.0" />
      <Label layoutX="404.0" layoutY="102.0" prefHeight="18.0" prefWidth="151.0" text="Email" />
      <Label layoutX="404.0" layoutY="170.0" text="Password" />
      <Button fx:id="loginButton" layoutX="501.0" layoutY="234.0" mnemonicParsing="false" onAction="#onLoginButtonClick" text="Log In" />
      <Button fx:id="signupButton" layoutX="42.0" layoutY="234.0" mnemonicParsing="false" onAction="#onSignupButtonClick" text="Sign Up" />
      <Label layoutX="42.0" layoutY="203.0" text="Don't have an account?Sign up here!" />
   </children>
</AnchorPane>
